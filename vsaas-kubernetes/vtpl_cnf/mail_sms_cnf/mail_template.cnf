mail.enterprise.registration.subject = Welcome To VSaaS

mail.enterprise.registration.body = <!DOCTYPE html><html lang='en'><head><meta charset='UTF-8'><meta name='viewport' content='width=device-width,initial-scale=1'><title>Welcome to Our Platform</title><style>body{ font-family: Arial, sans-serif; line-height: 1.6; margin: 0; padding: 0; display: flex; justify-content: center; align-items: center; min-height: 100vh;} .container{ max-width: 600px; padding: 20px; border: 1px solid #ccc; border-radius: 5px; text-align: center;} .header{ margin-bottom: 20px;} h2{ color: #333;} .main-message{ text-align: left;} p{ color: #555;} .logo{ max-width: 80%; height: auto; margin-bottom: 20px;} .footer{ margin-top: 20px; padding-top: 10px; border-top: 1px solid #ccc; text-align: left;} .footer p{ margin: 0; color: #777; font-size: 12px;} </style></head><body><div class='container'><div class='header'><img src='cid:cidlogo.png' alt='Logo' class='logo'><h2>Welcome to VSaaS</h2></div><div class='main-message'><p>Your enterprise, <strong>[enterpriseName]</strong>, has been successfully registered on VSaaS.</p></div><div class='footer'><p>Soterix Systems</p><p>3839 McKinney Ave, Suite 155 </p><p>Dallas, Texas, 75226, United States</p><p>www.soterixsystems.com</p></div></div></body></html>

mail.user.registration.subject = Welcome to VSaaS

mail.user.registration.body = <!DOCTYPE html><html lang='en'><head><meta charset='UTF-8'><meta name='viewport' content='width=device-width,initial-scale=1'><title>Welcome to Our Platform</title><style>body{ font-family: Arial, sans-serif; line-height: 1.6; margin: 0; padding: 0; display: flex; justify-content: center; align-items: center; min-height: 100vh;} .container{ max-width: 600px; padding: 20px; border: 1px solid #ccc; border-radius: 5px; text-align: center;} .header{ margin-bottom: 20px;} h2{ color: #333;} .main-message{ text-align: left;} p{ color: #555;} .logo{ max-width: 80%; height: auto; margin-bottom: 20px;} .cta-button, .cta-button:visited{ display: inline-block; padding: 10px 20px; background-color: #3498db; color: #fff; text-decoration: none; border-radius: 3px; margin-top: 20px;} .footer{ margin-top: 20px; padding-top: 10px; border-top: 1px solid #ccc; text-align: left;} .footer p{ margin: 0; color: #777; font-size: 12px;} </style></head><body><div class='container'><div class='header'><img src='cid:cidlogo.png' alt='Logo' class='logo'><h2>Welcome to VSaaS Platform!</h2></div><div class='main-message'><p>Hi [name],</p><p>Your account has been successfully created as a enterprise [userType] on [enterpriseName]. Here are your login details:</p><p><strong>Username: </strong>[username]</p><p><strong>Password: </strong>[password]</p><p>For security reasons, please change your password after your first login.</p></div><a href='https://vsaas-qa.videonetics.com/' class='cta-button'>Click to Continue</a><div class='footer'><p>Soterix Systems</p><p>3839 McKinney Ave, Suite 155 </p><p>Dallas, Texas, 75226, United States</p><p>www.soterixsystems.com</p></div></div></body></html>


mail.passwordchange.subject = Password Changed Successfully

mail.passwordchange.body = <!DOCTYPE html><html lang='en'><head><meta charset='UTF-8'><meta name='viewport' content='width=device-width,initial-scale=1'><title>Password Change Notification</title><style>body{ font-family: Arial, sans-serif; line-height: 1.6; margin: 0; padding: 0; display: flex; justify-content: center; align-items: center; min-height: 100vh;} .container{ max-width: 600px; padding: 20px; border: 1px solid #ccc; border-radius: 5px; text-align: center;} .header{ margin-bottom: 20px;} h2{ color: #333;} .main-message{ text-align: left;} p{ color: #555;} .logo{ max-width: 80%; height: auto; margin-bottom: 20px;} .cta-button{ display: inline-block; padding: 10px 20px; background-color: #3498db; color: #fff; text-decoration: none; border-radius: 3px; margin-top: 20px; cursor: pointer;} .footer{ margin-top: 20px; padding-top: 10px; border-top: 1px solid #ccc; text-align: left;} .footer p{ margin: 0; color: #777; font-size: 12px;} </style></head><body><div class='container'><div class='header'><img src='cid:cidlogo.png' alt='Logo' class='logo'><h2>Password Changed Successfully</h2></div><div class='main-message'><p>Hi, [name]</p><p>Your password for your account has been successfully changed.</p><p>If you did not initiate this change, please contact admin immediately.</p></div><a href='https://vsaas-qa.videonetics.com/' class='cta-button'>Contact Admin</a><div class='footer'><p>Soterix Systems</p><p>3839 McKinney Ave, Suite 155 </p><p>Dallas, Texas, 75226, United States</p><p>www.soterixsystems.com</p></div></div></body></html>


